<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deadswine.views">


    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value="YOUR_API_KEY"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FabMorphActivity"
            android:label="Sample: Fab Morph"
            />

        <activity android:name=".FabMorphGeocoderActivity"
            android:label="Sample: Fab Morph Advanced + geocoder"
            />

        <activity android:name=".SampleSlidingPanel.SlidingPanelActivity"
            android:label="Sample: Sliding Panel"
            />


    </application>

</manifest>
